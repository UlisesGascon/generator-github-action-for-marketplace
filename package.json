{
  "name": "generator-github-action-for-marketplace",
  "version": "0.0.0",
  "description": "A yeoman-based template to generate Github Actions ready for the marketplace",
  "homepage": "https://github.com/UlisesGascon/generator-github-action-for-marketplace",
  "author": {
    "name": "Ulises Gascon",
    "url": "https://ulisesgascon.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman",
    "yeoman-generator",
    "github-actions"
  ],
  "devDependencies": {
    "jest": "26.1.0",
    "prettier-standard": "16.4.1",
    "standard": "16.0.4",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.7.0"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": [
      "coverage/**",
      "generators/app/templates/**"
    ]
  },
  "engines": {
    "npm": ">= 14.0.0"
  },
  "dependencies": {
    "chalk": "2.1.0",
    "slugify": "1.6.5",
    "yeoman-generator": "3.1.1",
    "yosay": "2.0.2"
  },
  "scripts": {
    "format:fix": "prettier-standard --format",
    "format": "prettier-standard --check",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "test": "jest --detectOpenHandles --forceExit  --verbose ./__tests__",
    "test:coverage": "jest --detectOpenHandles --forceExit --verbose --coverage ./__tests__",
    "test:watch": "jest --detectOpenHandles --forceExit --verbose --watchAll ./__tests__"
  },
  "repository": "https://github.com/UlisesGascon/generator-github-action-for-marketplace",
  "license": "MIT"
}
